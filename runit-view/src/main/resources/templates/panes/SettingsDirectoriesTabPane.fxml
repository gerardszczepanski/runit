<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<Pane xmlns:fx="http://javafx.com/fxml/1" fx:id="settingsGeneralTabPane" prefHeight="403.0" prefWidth="584.0"
      style="-fx-background-color: #D3CAC2;" xmlns="http://javafx.com/javafx/8.0.111">
    <children>
        <Label layoutX="14.0" layoutY="20.0" prefHeight="21.0" prefWidth="156.0" text="Directory Aliases"/>
        <Button layoutX="14.0" layoutY="276.0" mnemonicParsing="false" onAction="#handleAddDirAlias" prefHeight="31.0"
                prefWidth="52.0" text="+"/>
        <Button layoutX="73.0" layoutY="276.0" mnemonicParsing="false" onAction="#handleRemoveDirAlias"
                prefHeight="31.0" prefWidth="52.0" text="-"/>
        <Button layoutX="431.0" layoutY="344.0" mnemonicParsing="false" onAction="#handleSaveTab" prefHeight="31.0"
                prefWidth="78.0" text="Save"/>
        <Button fx:id="cancelButton" layoutX="509.0" layoutY="344.0" mnemonicParsing="false"
                onAction="#handleCancelButton" prefHeight="31.0" prefWidth="78.0" text="Cancel"/>
        <TableView fx:id="dirAliasTableView" layoutX="14.0" layoutY="49.0" prefHeight="227.0" prefWidth="573.0">
            <columns>
                <TableColumn fx:id="aliasColumn" prefWidth="196.0" text="Alias"/>
                <TableColumn fx:id="directoryPathColumn" prefWidth="376.0" text="Directory"/>
            </columns>
        </TableView>
    </children>
</Pane>
